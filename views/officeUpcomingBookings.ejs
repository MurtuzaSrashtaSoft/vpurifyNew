<html>
    <link type="text/css" id="dark-mode" rel="stylesheet" href=""><style type="text/css" id="dark-mode-custom-style"></style>
    <head>
        
        
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>Title Here</title>

    <link href="/css/font-awesome.min.css" rel="stylesheet">
	<link rel="stylesheet" href="/css/style-starter.css">

    <link href="/css/style.css" rel="stylesheet" type="text/css" media="all">
	<link href="/css/styles.css" rel="stylesheet" type="text/css" media="all">
	<script src="https://kit.fontawesome.com/42e954c897.js" crossorigin="anonymous"></script>

    <link href="//fonts.googleapis.com/css?family=Montserrat:300,400,500,600" rel="stylesheet">
    <link href="//fonts.googleapis.com/css?family=Open+Sans:400,600,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=M+PLUS+Rounded+1c:wght@500&display=swap" rel="stylesheet">
    <link href="/css/table.css" rel="stylesheet">
    </head>
    

    <body>
        <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="/css/sidebar.css">
            <div class="w3-sidebar w3-bar-block" style="display:none" id="mySidebar">
    
                <button onclick="w3_close()" class="w3-bar-item w3-button w3-large">Close &times;</button>
                <a href="#" class="w3-bar-item w3-button">Link 1</a>
                <a href="#" class="w3-bar-item w3-button">Link 2</a>
                <a href="#" class="w3-bar-item w3-button">Link 3</a>
              </div>
              <script>
                function w3_open() {
                  document.getElementById("mySidebar").style.width = "25%";
                  document.getElementById("mySidebar").style.display = "block";
                }
                
                function w3_close() {
                  document.getElementById("mySidebar").style.display = "none";
                }
                </script>
              
            <button style="position: relative;" class="w3-button w3-teal w3-xlarge" onclick="w3_open()">â˜° Menu</button>
        <img src="\images\logo_t.png" alt="red" style="max-width: 33%; top: 0px; margin-right:auto;  margin-left: auto; display: block;
    position: relative;min-width:30%;z-index: 1;"><img src="\images\blue.png" alt="blue" style="position: fixed;
        bottom: 0px;
        left: 0px;
        
        z-index: -1;
        padding: 0px;
        margin: 0px;"> 
    <img src="\images\red_car.png" alt="red" style="position: fixed;
    top: 0px;
    right: 0px;
    max-width: 90%;
    max-height: 90%;
    z-index: -1;
    padding: 0px;
    margin: 0px;">
   
        
    <div id="page" class="page">
        
		<div class="main-titles-head text-center">
			<h3 class="header-name">
				Pending Office Bookings
			</h3> 
		</div>
    
    <div class="two-grids">
        <div class="mid-class">
            
            
            <div class="txt-left-side editContent" style="border-radius: 40px ;color: rgb(0, 0, 0); font-size: 16px; background-color: rgb(255, 255, 255); font-family: &quot;Open Sans&quot;, sans-serif;">
				<% for(var i=0;i<service.officePendings.length;i++){ %>
                
					<a href="#page"><img class="img-responsive" src="https://vpurify.s3.us-east-2.amazonaws.com/<%= service.officePendings[i].userQR %>" alt=" "></a>

                <table class="table-fill">
                    <thead>
                    <tr>
                    <th class="text-left">Time</th>
                    <th class="text-left"><%= service.officePendings[i].slot %></th>
                    </tr>
                    </thead>
                    <tbody class="table-hover">
                    <tr>
                    <td class="text-left">Date</td>
                    <td class="text-left"><%= service.officePendings[i].date %></td>
                    </tr>
                    <tr>
                    <td class="text-left">Customer Name</td>
                    <td class="text-left"><%= service.officePendings[i].name %></td>
                    </tr>
                    <tr>
                    <td class="text-left">Customer Email</td>
                    <td class="text-left"><%= service.officePendings[i].email %></td>
                    </tr>
                    <tr>
                    <td class="text-left">Contact</td>
                    <td class="text-left"><%= service.officePendings[i].phone %></td>
                    </tr>
                    <tr>
                    <td class="text-left">Vehicle Number</td>
                    <td class="text-left"><%= service.officePendings[i].vehicleNo %></td>
                    </tr>
                    </tbody>
                    </table>
                    <br>
            <br>
                <form method="post">
					<input type="hidden" name="name" value="<%= service.officePendings[i].name %>">
				    <input type="hidden" name="phone" value="<%= service.officePendings[i].phone %>">
					<input type="hidden" name="email" value="<%= service.officePendings[i].email %>">
					<input type="hidden" name="slot" value="<%= service.officePendings[i].slot %>">
					<input type="hidden" name="date" value="<%= service.officePendings[i].date %>">
					<input type="hidden" name="vehicleNo" value="<%= service.officePendings[i].vehicleNo %>">
											
                    <div class="form-left-to editContent">
						
                        <span class="fa fa-user-o" aria-hidden="true" style="outline: none; cursor: inherit;">Select</span>
                        <select type="text" name="employee"  required="">
							<% for(var j=0;j<service.officeEmployees.length;j++){ %>
							<option value="<%= service.officeEmployees[j].name %>"><%= service.homeEmployees[j].name %> </option>
							<% } %> 
							</select>
						

                        <div class="clear"></div>
                    </div>
                    
                    <div class="btnn">
                        <button type="submit" class=" btn" style="outline: none; cursor: inherit;">Submit</button>
                    </div>
                </form>
               
				<div class="clear"></div>
				<% } %>
            </div>
        </div>
    
    
	  </div></div>


    </body>
</html>

	 