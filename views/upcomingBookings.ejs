<!doctype html>
<html lang="en">
<head>
	<meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>DashBoard</title>
	<link rel="stylesheet" href="/css/style-starter.css">
	<link href="/css/font-awesome.min.css" rel="stylesheet">
	<link rel="stylesheet" href="/css/sidebar.css">
    <link href="/css/style.css" rel="stylesheet" type="text/css" media="all">
    <link href="//fonts.googleapis.com/css?family=Montserrat:300,400,500,600" rel="stylesheet">
    <link href="//fonts.googleapis.com/css?family=Open+Sans:400,600,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=M+PLUS+Rounded+1c:wght@500&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Muli:400,600,700&display=swap" rel="stylesheet">
</head>
<body id="home">
	  
	<!-- SideBar -->
	<div class="w3-sidebar w3-bar-block" style="display:none" id="mySidebar">
		<button onclick="w3_close()" class="w3-bar-item w3-button w3-large">Close &times;</button>
		<a href="#" class="w3-bar-item w3-button">Link 1</a>
		<a href="#" class="w3-bar-item w3-button">Link 2</a>
		<a href="#" class="w3-bar-item w3-button">Link 3</a>
	</div>
	<script>
		function w3_open() {
			document.getElementById("mySidebar").style.width = "25%";
			document.getElementById("mySidebar").style.display = "block";
			}
		   
		function w3_close() {
			document.getElementById("mySidebar").style.display = "none";
			}
	</script>
	<button style="position: absolute;z-index:0; border:none" class="w3-button w3-teal w3-xlarge" onclick="w3_open()">â˜° Menu</button>
	<!--/SideBar -->

	<!-- Logout Button -->
	<div class="topnav" style="position:static; z-index:2;">
		<div class="topnav-right" style="text-align: right;">  
			<a href="#admin">Logout</a>	
		</div>
	</div>
	<!-- /Logout Button -->

	<!-- Background Images and Logo -->
	<img src="/images/logo_t.png" alt="red" 
		style="max-width: 33%; top: 0px; 
		margin-right:auto;  margin-left: auto; 
		display: block;position: relative;
		min-width:30%;z-index: 1;">
	<img src="/images/blue.png" alt="blue" 
		style="position: fixed; bottom: 0px;
    	left: 0px;z-index: -1;
        padding: 0px;margin: 0px;">
	<img src="/images/red_car.png" alt="red" 
		style="position: fixed;top: 0px;
    	right: 0px;max-width: 90%;
    	max-height: 90%;z-index: -1;
    	padding: 0px;margin: 0px;">
    <!--/Background Images and Logo -->

    <section class="w3l-team-main-6">
		<div class="container">
            <div class="main-titles-head text-center">
                <h3 class="header-name">
					Pending Home Bookings
                </h3> 
            </div>
			<div class="row grid-col-4 text-center">
                <% for(var i=0;i<service.homePendings.length;i++){ %>
                <div class="team-colomn col-lg-3 col-md-6 col-sm-6">	
					<div class="team-main-6">
						<div class="right-team-9">
							
                            <a href="#page"><img class="img-responsive" src="https://vpurify.s3.us-east-2.amazonaws.com/<%= service.homePendings[i].userQR %>" alt=" "></a>
							<h4 class="sm-text-32">Customer Name: <%= service.homePendings[i].name %></h4>
                            <p class="sm-text-32">Customer Email: <%= service.homePendings[i].email %> </p>
                            <p class="sm-text-32">Customer Contact: <%= service.homePendings[i].phone %> </p>
                            <p class="sm-text-32">Customer Vehicle Number: <%= service.homePendings[i].vehicleNo %> </p>
                            <p class="sm-text-32">Date: <%= service.homePendings[i].date %> </p>
							<p class="sm-text-32">Time Slot: <%= service.homePendings[i].slot %> </p>
							<br>						
						    <div class="two-grids">
								<div class="mid-class">
									
									<div class="txt-left-side editContent" style="border-radius: 40px ;color: rgb(0, 0, 0); font-size: 16px; background-color: rgb(255, 255, 255); font-family: &quot;Open Sans&quot;, sans-serif;">
										<h2 class="editContent"> Assign Employee </h2>
										<form method="POST">
											<input type="hidden" name="name" value="<%= service.homePendings[i].name %>">
											<input type="hidden" name="phone" value="<%= service.homePendings[i].phone %>">
											<input type="hidden" name="email" value="<%= service.homePendings[i].email %>">
											<input type="hidden" name="slot" value="<%= service.homePendings[i].slot %>">
											<input type="hidden" name="date" value="<%= service.homePendings[i].date %>">
											<input type="hidden" name="vehicleNo" value="<%= service.homePendings[i].vehicleNo %>">
											<div class="form-left-to editContent">
												<span class="fas fa-map-marker-alt" aria-hidden="true" style="outline: none; cursor: inherit;"></span>
												<select name="employee">
													<% for(var i=0;i<service.homeEmployees.length;i++){ %>
													<option value="<%= service.homeEmployees[i].name %>"><%= service.homeEmployees[i].name %> </option>
													<% } %> 
												</select>
						
												<div class="clear"></div>
											</div>
											

											
											
										   
											<div class="btnn">
												<button type="submit" style="color: violet;" class=" btn" style="outline: none; cursor: inherit;">Assign</button>
											</div>
										</form>
										
										
									</div>
								</div>
							
						</div>	
					</div>
                </div>
                 <% } %> 
			</div>
		</div>
	
	</section>
    		
		
    

				</div>
			</div>
		</div>
	</section>