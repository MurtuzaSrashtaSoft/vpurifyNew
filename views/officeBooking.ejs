<br>
<div class="mid-class">
    <div class="txt-left-side editContent" style="border-radius: 40px ;color: rgb(0, 0, 0); font-size: 16px; background-color: rgb(255, 255, 255); font-family: &quot;Open Sans&quot;, sans-serif;">
                
        <h2 class="editContent">Make Booking</h2>
        
        <form method="POST" enctype="multipart/form-data">
            <input type="hidden" value="<%= service.firm %>" readonly name="detailsName">
    <input type="hidden" value="<%= service.phone %>" readonly name="detailsPhone">
    <input type="hidden" value="<%= service.email %>" readonly name="detailsEmail">
    <input type="hidden" value="<%= slotDate %>" readonly name="detailsDate">
    <input type="hidden" value="<%= slotTime %>" readonly name="detailsSlot">
    <input type="hidden" value="<%= service.city %>" readonly name="detailsCity">
            <div class="form-left-to editContent">
                <span class="fas fa-map-marker-alt" aria-hidden="true" style="outline: none; cursor: inherit;"></span>
                <input type="email" name="yourEmail" placeholder="Please enter registered email id">

                <div class="clear"></div>
            </div>
            <div class="form-left-to editContent">
                <span class="fas fa-map-marker-alt" aria-hidden="true" style="outline: none; cursor: inherit;"></span>
                <input type="text" name="userName" placeholder="Please enter registered Name">

                <div class="clear"></div>
            </div>
            <div class="form-left-to editContent">
                <span class="fas fa-map-marker-alt" aria-hidden="true" style="outline: none; cursor: inherit;"></span>
                <input type="text" name="phone" placeholder="Please enter registered contact number">

                <div class="clear"></div>
            </div>
            <div class="form-left-to editContent">
                <span class="fas fa-map-marker-alt" aria-hidden="true" style="outline: none; cursor: inherit;"></span>
                <input type="text" name="vehicleNo" placeholder="Please enter registered Vehicle Number">

                <div class="clear"></div>
            </div>
            <div class="form-left-to editContent">
                <span class="fas fa-map-marker-alt" aria-hidden="true" style="outline: none; cursor: inherit;"></span>
                <select type="text" name="model" placeholder="Please enter registered Vehicle Model">
                    <option value="Two Wheeler">Two Wheeler</option>
                    <option value="Sedan">Sedan</option>
                    <option value="Hatchback">Hatchback</option>
                    <option value="Auto Rikshaw">Auto Rikshaw</option>
                    <option value="small">Small Commercial</option>
                    <option value="Multi Axel">Multi Axel</option>
                    <option value="SUV">SUV</option>
                    <option value="Maxi">Maxi</option>
                    <option value="Tempo">Tempo</option>
                    <option value="Bus">Bus</option>
                    <option value="others">Others</option>
                    </select>

                <div class="clear"></div>
            </div>
            <div class="form-left-to editContent">
                <span class="fas fa-map-marker-alt" aria-hidden="true" style="outline: none; cursor: inherit;">Your QR code</span>
                
                <input type="file" name="img" accept="image/*" placeholder="Please enter registered Vehicle Number">

                <div class="clear"></div>
            </div>
            
           
            <div class="btnn">
                <button type="submit" class=" btn" style="outline: none; cursor: inherit;">Book Slot</button>
            </div>
        </form>
        
    </div>
    </div>


        </div>



   
