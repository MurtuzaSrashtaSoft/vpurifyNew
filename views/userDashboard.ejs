<!doctype html>
<html lang="en">
<head>
	<meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>DashBoard</title>
	<link rel="stylesheet" href="/css/style-starter.css">
	<link rel="stylesheet" href="/css/table.css">
	<link href="/css/font-awesome.min.css" rel="stylesheet">
	<link rel="stylesheet" href="/css/sidebar.css">
    <link href="/css/style.css" rel="stylesheet" type="text/css" media="all">
    <link href="//fonts.googleapis.com/css?family=Montserrat:300,400,500,600" rel="stylesheet">
    <link href="//fonts.googleapis.com/css?family=Open+Sans:400,600,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=M+PLUS+Rounded+1c:wght@500&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Muli:400,600,700&display=swap" rel="stylesheet">
</head>
<body id="home">
	  
	<!-- SideBar -->
	<div class="w3-sidebar w3-bar-block" style="display:none" id="mySidebar">
		<button onclick="w3_close()" class="w3-bar-item w3-button w3-large">Close &times;</button>
		<a href="/subscription/<%= user._id %>" class="w3-bar-item w3-button">Buy Coins</a>
		<a href="/search/<%= user._id %>" class="w3-bar-item w3-button">Search Stations</a>
		<a href="/qrCode/<%= user._id %>" class="w3-bar-item w3-button">Your QR code</a>
		<a href="/completedUser/<%= user._id %>" class="w3-bar-item w3-button">Completed Bookings</a>
		<a href="/lastDetails/<%= user._id %>" class="w3-bar-item w3-button">Last sanitisation details</a>
	</div>
	<script>
		function w3_open() {
			document.getElementById("mySidebar").style.width = "25%";
			document.getElementById("mySidebar").style.display = "block";
			}
		   
		function w3_close() {
			document.getElementById("mySidebar").style.display = "none";
			}
	</script>
	<button style="position: absolute;z-index:0; border:none" class="w3-button w3-teal w3-xlarge" onclick="w3_open()">â˜° Menu</button>
	<!--/SideBar -->

	<!-- Logout Button -->
	<div class="topnav" style="position:static; z-index:2;">
		<div class="topnav-right" style="text-align: right;">  
			<a href="/">Logout</a>	
		</div>
	</div>
	<!-- /Logout Button -->

	<!-- Background Images and Logo -->
	<img src="/images/logo_t.png" alt="red" 
		style="max-width: 33%; top: 0px; 
		margin-right:auto;  margin-left: auto; 
		display: block;position: relative;
		min-width:30%;z-index: 1;">
	<img src="/images/blue.png" alt="blue" 
		style="position: fixed; bottom: 0px;
    	left: 0px;z-index: -1;
        padding: 0px;margin: 0px;">
	<img src="/images/red_car.png" alt="red" 
		style="position: fixed;top: 0px;
    	right: 0px;max-width: 90%;
    	max-height: 90%;z-index: -1;
    	padding: 0px;margin: 0px;">
	<!--/Background Images and Logo -->
	
	<!-- Profile -->
	<section class="w3l-team-main-6">
		<div class="container">
            <div class="main-titles-head text-center">
                <h3 class="header-name">
					My Profile
                </h3> 
            </div>
			<div class="row grid-col-4 text-center">
                <div class="team-colomn col-lg-3 col-md-6 col-sm-6">	
					<div class="team-main-6">
						<div class="right-team-9">
							<a href="#page"><img class="img-responsive" src="https://vpurify.s3.us-east-2.amazonaws.com/<%= user.profile %> " alt=" "></a>
							<h4 class="sm-text-32">Name: <%= user.name %></h4>
                            <p class="sm-text-32">Email: <%= user.email %> </p>
                            <p class="sm-text-32">Phone Number: <%= user.email %> </p>
                        	
						</div>	
					</div>
				</div>
			</div>
		</div>
	
	</section>
	<!-- /Profile -->

	<!-- Text Cards -->
	<section class="w3l-recent-work-hobbies">
   		<div class="recent-work ">
   			<div class="container">
				<div class="row about-about">
					<div class="col-lg-4 col-md-6 about-line-top">
						<div class="about-grids">
							<h4>Vehicle Informations</h4>
							<div class="mt-3 about-list">
								<ul>
									<li ><span class="fa fa-chevron-circle-right" aria-hidden="true"></span>
										<p class="para"><b>Vehicle Type: </b><%= user.vehicleType %> </p>
									</li>
									<li><span class="fa fa-chevron-circle-right" aria-hidden="true"></span>
										<p class="para"><b>Vehicle Company: </b> <%= user.vehicleCompany %> </p>
									</li>
									<li><span class="fa fa-chevron-circle-right" aria-hidden="true"></span>
										<p class="para"><b>Model: </b> <%= user.model %> </p>
                                    </li>
                                    <li><span class="fa fa-chevron-circle-right" aria-hidden="true"></span>
										<p class="para"><b>Vehicle Number:</b> <%= user.number %> </p>
                                    </li>
                                    <li><span class="fa fa-chevron-circle-right" aria-hidden="true"></span>
										<p class="para"><b>Passing Year:</b> <%= user.year %> </p>
									</li>
								</ul>
							</div>
						</div>
					</div>
					
					
				</div>
   			</div>
   		</div>
	</section>
	<!--/ Text Cards -->
	
	<!-- Bar with text and numbers -->
	<section class="w3l-about ">
		<div class="skills-bars text-center">
 			<div class="container">
				<div style="display: block; width: auto;" class="row counter-width">
					<div class="counter-counter ">
						<h2 style="text-align: center;">Coins in my account</h2>
						<span><%= user.coins %> </span>
					</div>
 					
 				</div>
			</div>
		</div>
	</section>
	<!-- Bar with text and numbers -->

	<!-- Documents -->
	
					
	<!-- Time Slots -->
	<section class="w3l-recent-work-hobbies">
		<div class="recent-work ">
			<div class="container">
				<div class="main-titles-head">
						<h3 class="header-name">Upcoming Home Bookings
						</h3>			  
				</div>
				<div class="row about-about">
					<% for(var i=((user.homeBookings.length)-1);i>=0;i--){ %>
					<div class="col-lg-4 col-md-6"style="border: rgba(256, 256, 256,0.01) solid 5.5px;">
						<div class="about-grids">
							
							<div class="mt-3 about-list">
								<table class="table-fill">
									<thead>
										<tr>
											<th class="text-left">Time</th>
											<th class="text-left"><%= user.homeBookings[i].slot %></th>
										</tr>
									</thead>
									<tbody class="table-hover">
										<tr>
											<td class="text-left">Date</td>
											<td class="text-left"><%= user.homeBookings[i].date %></td>
										</tr>
										<tr>
											<td class="text-left">Station Name</td>
											<td class="text-left"><%= user.homeBookings[i].name %></td>
										</tr>
										<tr>
											<td class="text-left">Station Email</td>
											<td class="text-left"><%= user.homeBookings[i].email %></td>
										</tr>
										<tr>
											<td class="text-left">Station Contact</td>
											<td class="text-left"><%= user.homeBookings[i].phone %></td>
										</tr>
										<tr>
											<td class="text-left">Station City</td>
											<td class="text-left"><%= user.homeBookings[i].city %></td>
										</tr>
										<tr>
											<th class="text-left">Coins</th>
											<th class="text-left"><%= user.homeBookings[i].coins %></th>
										</tr>
									</tbody>
								</table>
							</div>
						</div>
					</div>
				<% } %> 
					</div>
					</div>
					</div>
	</section>

	
	
    <!-- /Time Slots -->
    <section class="w3l-recent-work-hobbies">
		<div class="recent-work ">
			<div class="container">
				<div class="main-titles-head">
						<h3 class="header-name">Upcoming At Station Bookings
						</h3>			  
				</div>
				<div class="row about-about">
					<% for(var i=((user.officeBookings.length)-1);i>=0;i--){ %>
					<div class="col-lg-4 col-md-6"style="border: rgba(256, 256, 256,0.01) solid 5.5px;">
						<div class="about-grids">
							
							<div class="mt-3 about-list">
								<table class="table-fill">
									<thead>
										<tr>
											<th class="text-left">Time</th>
											<th class="text-left"><%= user.officeBookings[i].slot %></th>
										</tr>
									</thead>
									<tbody class="table-hover">
										<tr>
											<td class="text-left">Date</td>
											<td class="text-left"><%= user.officeBookings[i].date %></td>
										</tr>
										<tr>
											<td class="text-left">Station Name</td>
											<td class="text-left"><%= user.officeBookings[i].name %></td>
										</tr>
										<tr>
											<td class="text-left">Station Email</td>
											<td class="text-left"><%= user.officeBookings[i].email %></td>
										</tr>
										<tr>
											<td class="text-left">Station Address</td>
											<td class="text-left"><%= user.officeBookings[i].address %></td>
										</tr>
										<tr>
											<td class="text-left">Station Contact</td>
											<td class="text-left"><%= user.officeBookings[i].phone %></td>
										</tr>
										<tr>
											<td class="text-left">Station City</td>
											<td class="text-left"><%= user.officeBookings[i].city %></td>
										</tr>
									</tbody>
								</table>
							</div>
						</div>
					</div>
				<% } %> 
					</div>
					</div>
					</div>
	</section>
</body>

</html>










